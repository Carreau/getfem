.NOTPARALLEL: $(AUTO_M_FILES) $(M_FILES)

%.m: ../%.cc
	@./../../bin/extract_mdoc $<

clean-m-files:
	@echo "cleaning generated m-files"
	rm -f $(AUTO_M_FILES)

clean-local: #clean-m-files
	rm -f gf_matlab
