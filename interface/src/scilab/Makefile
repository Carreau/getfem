install-toolboxSCRIPTS: $(toolbox_SCRIPTS)
	@$(NORMAL_INSTALL)
	test -z "$(toolboxdir)" || $(MKDIR_P) "$(DESTDIR)$(toolboxdir)"
	@list='$(toolbox_SCRIPTS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(toolboxSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(toolboxdir)/$$f'"; \
	    $(toolboxSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(toolboxdir)/$$f"; \
	  else :; fi; \
	done

uninstall-toolboxSCRIPTS:
	@$(NORMAL_UNINSTALL)
	@list='$(toolbox_SCRIPTS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  echo " rm -f '$(DESTDIR)$(toolboxdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(toolboxdir)/$$f"; \
	done

