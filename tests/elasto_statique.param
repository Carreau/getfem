%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% parameters for program elasto_statique                                  %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%% pde parameters :	        				      %%%%%
N = 2;                  % dimension of the domain.
PG = 9.81; 		% constant of gravitation.
% G = 77.0;	        % Lamé coefficient.
% LAMBDA = 107.0;   	% Lamé coefficient.
G = 1.0;	        % Lamé coefficient.
LAMBDA = 1.0;   	% Lamé coefficient.
LX = 1.0;		% size in X.
LY = 1.0;	        % size in Y.
LZ = 1.0;		% size in Z.
FT = 0.2;               % parameter for the exact solution.  
%FT = 3.141593;

%%%%%   discretisation parameters  :                     	      %%%%%
K = 2;                  % Finite element degree.
INTEGRATION = 0;        % 0 = exact integration
			% 1 = Newton Cotes of degree 2 * K
NX = 5;            	% space steps.
RESIDU = 1E-9;     	% residu for conjugate gradient.


%%%%%   saving parameters                                             %%%%%
ROOTFILENAME = 'elasto_statique';  % Root of data files.

