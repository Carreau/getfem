%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% parameters for program laplacian                                        %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%% pde parameters :	        				      %%%%%
N = 1;                  % dimension of the domain.
LX = 1.0;		% size in X.
LY = 1.0;	        % size in Y.
LZ = 1.0;		% size in Z.
FT = 0.1;               % parameter for the exact solution.

%%%%%   discretisation parameters  :                     	      %%%%%
MESH_TYPE = 0;          % 0 = simplexes
			% 1 = parallelepipeds
			% 2 = prisms
INCLINE = 0.0;          % Incline of the mesh.
K = 1;                  % Finite element degree.
INTEGRATION = 1;        % 0 = exact integration.
			% 1 = Newton Cotes of degree 2 * K
			% 2 = Product of 1D Gauss for parallelepipeds.
			% 11, 12, 13, 14, 15, 16, 17 triangle(n-10)
			% 21, 22, 23, 25 tetrahedron(n-20)
			% 32, 33, 35 quadrilateral(n-30)
NX = 10;            	% space step.
RESIDU = 1E-9;     	% residu for conjugate gradient.
MIXTEHYBRID = 'N';

%%%%%   saving parameters                                             %%%%%
ROOTFILENAME = 'laplacian';     % Root of data files.

