%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% parameters for program laplacien                                        %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%% pde parameters :	        				      %%%%%
N = 3;                  % dimension of the domain.
LX = 1.0;		% size in X.
LY = 1.0;	        % size in Y.
LZ = 1.0;		% size in Z.
FT = 1.0;               % parameter for the exact solution.
% FT = 3.141593;

%%%%%   discretisation parameters  :                     	      %%%%%
K = 2;                  % Finite element degree.
INTEGRATION = 25;        % 0 = exact integration.
			% 1 = Newton Cotes of degree 2 * K
			% 11, 12, 13, 14, 15, 16, 17 triangle(n-10)
			% 21, 22, 23, 25 tetrahedron(n-20)
NX = 10;            	% space steps.
RESIDU = 1E-9;     	% residu for conjugate gradient.
MIXTEHYBRID = 'N';


%%%%%   saving parameters                                             %%%%%
ROOTFILENAME = 'laplacien';     % Root of data files.

