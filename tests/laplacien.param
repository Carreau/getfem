%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% parametres du programme elastostatique                                  %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%% parametres physiques :					      %%%%%

N = 1;                  % dimension du probleme.
LX = 1.0;		% taille en X.
LY = 1.0;	        % taille en Y.
LZ = 1.0;		% taille en Z.
FT = 1.0;               % parametre pour la solution exacte.
% FT = 3.141593;

%%%%%   parametres de discretisation :  			      %%%%%
K = 2;                  % Degre de l'element fini de Lagrange.
INTEGRATION = 0;        % 0 = integration exacte
			% 1 = Newton Cotes de degré 2 * K
NX = 20;            	% Nombre de pas d'espace.
ITEIMPL = 1;            % nombre d'iteration de point fixe.
RESIDU = 1E-9;     	% Test d'arret pour la methode iterative
MIXTE = 'N';


%%%%%   parametres lies a la sauvegarde                               %%%%%
ROOTFILENAME = 'laplacien';     % Racine des fichiers de donnees.

